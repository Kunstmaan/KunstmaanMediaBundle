	<ul class="breadcrumb">
		<li><a href="{{ path('KunstmaanAdminBundle_homepage') }}">{{ 'media.menu.home'|trans }}</a> <span class="divider">/</span></li>
        <li><a href="{{ path('KunstmaanMediaBundle_media') }}">{{ 'media.menu.media'|trans }}</a> <span class="divider">/</span></li>
        {% block subbreadcrumb %}
			
        {% endblock %}
