{% extends 'KunstmaanAdminBundle:Default:layout.html.twig' %}

{% block sidebar %}
<div class="sidebar">
    <ul>
        <li class="header">{{ 'media.menu.media'|trans }}</li>
        <li><a href="{{ path('KunstmaanMediaBundle_media_images') }}">{{ 'media.menu.images'|trans }}</a></li>
        <li><a href="{{ path('KunstmaanMediaBundle_media_videos') }}">{{ 'media.menu.videos'|trans }}</a></li>
        <li><a href="{{ path('KunstmaanMediaBundle_media_slides') }}">{{ 'media.menu.slides'|trans }}</a></li>
