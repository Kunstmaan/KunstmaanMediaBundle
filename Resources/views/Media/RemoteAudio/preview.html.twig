{% set handler = mediamanager.getHandler(media) %}
{% set helper = handler.getFormHelper(media) %}
{% if helper.type == 'soundcloud' %}
<div class="audio-pp soundcloud-pp">
    <iframe width="560" height="315" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/{{ helper.code }}&amp;auto_play=false&amp;hide_related=false&amp;visual=true"></iframe>
</div>
{% endif %}
