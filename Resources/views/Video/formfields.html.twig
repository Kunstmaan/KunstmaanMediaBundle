{{ form_errors(form) }}
        <div class="clearfix">
            <label for="xlInput3">Name</label>
            <div class="input">
                {{ form_widget(form.name) }}
            </div>
        </div>
        <div class="clearfix">
            {{ form_errors(form) }}
            <label for="xlInput3">Video code</label>
            <div class="input">
                {{ form_widget(form.content) }}
            </div>

        </div>
        <div class="clearfix">
            <label for="xlInput3">Type</label>
            <div class="input">
                {{ form_widget(form.type) }}
            </div>
        </div>
        {{ form_rest(form) }}